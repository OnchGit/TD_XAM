<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FourSquare.Auxilliary.PlaceViewCell">
    <ViewCell.View>
        
            <StackLayout Orientation="Horizontal" HeightRequest="100" Spacing="0" Padding="0">
                <Image Source="{Binding ImgSrc}" HeightRequest="100" WidthRequest="100"/>
                <StackLayout>
                <Label Text="{Binding Title}" Margin="0,2,0,2"/>
                <Label Text="{Binding Description}" Margin="0,0,0,3"/>
            </StackLayout>
                
                
                

            </StackLayout>
        
    </ViewCell.View>
</ViewCell>